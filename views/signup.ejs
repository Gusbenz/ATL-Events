<body class="signup_page">

 <h2>Create a new account</h2>
  <form id="local-reg" action="/signup" method="post">
    <div>
        <p></p>
        <label>Email:</label>
        <input type="text" style="color: black" name="email" id="email"/>
    </div>
    <div>
        <label>New Password:</label>
        <input type="password" style="color: black" name="password" id="password"/>
    </div>
    <div>
        <input type="submit" class="btn btn-primary btn-sm" value="Signup"/>
    </div>
  </form>

</body>

